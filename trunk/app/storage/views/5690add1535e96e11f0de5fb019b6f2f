<script type="text/javascript">
    $('body').show();
    NProgress.start();
    setTimeout(function() {
        NProgress.done();
        $('.fade').removeClass('out');
    }, 100);

    new UISearch(document.getElementById('sb-search'));
    // 
    function OpenPop(maskId, ElementId)
    {
        document.getElementById(maskId).style.display = 'block';
        document.getElementById(ElementId).style.display = 'block';
    }
    //
    function ClosePop(maskId, ElementId)
    {
        document.getElementById(maskId).style.display = 'none';
        document.getElementById(ElementId).style.display = 'none';
    }
    //
    function trim(str) {
        return str.replace(/^[\s]+/, "").replace(/[\s]+$/, "").replace(/[\s]{2,}/, " ");
    }

    function checkAll(bx, nameCheck) {
        var cbs = document.getElementsByName(nameCheck);
        for (var i = 0; i < cbs.length; i++) {
            if (cbs[i].name == nameCheck) {
                cbs[i].checked = bx.checked;
            }
        }
    }
    function PopUp(url){
    new_window = window.open(url,'In','resizable=0,height=600px,width=1024px,scrollbars=yes,rollbars=yes');
    if (window.focus)
        new_window.focus();
}
</script>